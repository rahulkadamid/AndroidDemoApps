<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:colorBackground">#1717e1</item>
    </style>

    <style name="AppTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:background">#d11a17</item>
        <item name="android:colorBackground">#3838c5</item>
    </style>

    <style name="AppTheme3" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:background">#f1bd13</item>
        <item name="android:colorBackground">#13e113</item>
    </style>

    <style name="AppTheme5" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:background">#be82cf</item>
        <item name="android:colorBackground">#faf8e6</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:background">#d8e8ed</item>
    </style>

</resources>
